{
  "version": 2,
  "name": "bismil-vinc",
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/admin",
      "dest": "/server.js"
    },
    {
      "src": "/mobilvinchizmeti",
      "dest": "/server.js"
    },
    {
      "src": "/insaatkurulumu",
      "dest": "/server.js"
    },
    {
      "src": "/petrolkuyuhizmeti",
      "dest": "/server.js"
    },
    {
      "src": "/petrolinsaatsahasi",
      "dest": "/server.js"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|ico|svg|woff|woff2|ttf|eot))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
} 